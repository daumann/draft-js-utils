  "version": "0.1.5",
  "description": "Collection of utilities for DraftJS",
  "main": "lib/main.js",
  "scripts": {
    "build": "babel src --ignore '_*' --out-dir lib",
    "lint": "eslint --max-warnings 0 .",
    "typecheck": "flow",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run typecheck && npm run test-src",
